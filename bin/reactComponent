#!/bin/bash

echo Creating new component template: Components/$1
mkdir Components/$1
touch Components/$1/$1.module.css
cat << EOF > Components/$1/index.js
import React from 'react';

import style from './$1.module.css';

export default function $1(props) {
  return (
    <div className={style.$1}>
      $1
    </div>
  );
}
EOF
