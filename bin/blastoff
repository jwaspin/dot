#!/bin/bash

SYS="/System/Applications/"
ROOT="/Applications/"

open_sys () {
    open "${SYS}$1"
}

open_root () {
    open "${ROOT}$1"
}

open_root_space () {
    open "${ROOT}$1 $2"
}

open_root_space2 () {
    open "${ROOT}$1 $2 $3"
}

open_sys Calendar.app
open_sys Mail.app
open_sys Messages.app
open_sys Notes.app
open_sys Reminders.app
open_sys Stocks.app

open_root Slack.app
open_root Trello.app

open_root_space Google Chrome.app
open_root_space 1Password 7.app

open_root_space2 Visual Studio Code.app

ssh-add -l

sleep 5
